<header>
  <nav>
    <a href="/listings">Website name</a>
  <% if (user.email) { %>
    <div>
      <div>Logged in as:
        <strong><%= user.email %></strong>
      </div>
      <form method="POST" action="/logout">
        <button type="submit">Logout</button>
      </form>
    </div>
  <% } else { %>
    <form method="GET" action="/login">
      <button type="submit">Login</button>
    </form>
  <% } %>
  </nav>
</header>
